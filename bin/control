#!/bin/bash

function start() {
  echo
}

function stop() {
  echo
}

function restart() {
    stop
    start
}

function status() {
  return 0
}

function reload() {
  echo
}

function tidy() {
  echo
}

case "$1" in
  start) start ;;
  stop) stop ;;
  restart) restart ;;
  status) status ;;
  reload) reload ;;
  tidy) tidy ;;
  *) exit 0
esac

